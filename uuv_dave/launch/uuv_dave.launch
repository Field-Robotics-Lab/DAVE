<!--
UUV launch for project DAVE
-->
<launch>
    <arg name="gui" default="true"/>
    <arg name="paused" default="false"/>
    <arg name="world_name" default="$(find uuv_dave)/worlds/uuv_dave_ocean_waves.world"/>

    <include file="$(find uuv_dave)/launch/uuv_dave_ocean_waves.launch">
        <arg name="paused" value="$(arg paused)"/>
        <arg name="gui" value="$(arg gui)"/>
        <arg name="world_name" value="$(arg world_name)"/>
    </include>

    <include file="$(find uuv_descriptions)/launch/upload_rexrov_oberon7.launch">
        <arg name="namespace" value="rexrov"/>
        <arg name="x" value="4"/>
        <arg name="y" value="4"/>
        <arg name="z" value="-94"/>
        <arg name="yaw" value="-1.8"/>
    </include>

    <include file="$(find uuv_control_cascaded_pid)/launch/joy_accel.launch">
        <arg name="model_name" value="rexrov"/>
    </include>

    <include file="$(find oberon7_control)/launch/joint_control.launch">
        <arg name="uuv_name" value="rexrov"/>
    </include>

    <node name="spawn_grabbable_bar" pkg="gazebo_ros" type="spawn_model" args="-file $(find uuv_dave)/models/bar/model.sdf -sdf -model grabbable_bar">
    </node>

</launch>
